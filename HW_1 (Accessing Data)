SELECT *
FROM products
WHERE product_name like 'N%' 
  AND unit_price > 50;
  
/*result 0 values*/



SELECT city, count(first_name)
FROM employees
GROUP BY city;

/*result 1 Redmond
Redmond 1
London 4
Tacoma 1
Kirkland 1
Seattle 2 */


SELECT company_name, city
FROM suppliers
WHERE company_name LIKE 'A%'
AND city = 'London';

/*result - 0 values */

select country, count(customer_id) as "Owner"
from customers 
where country = 'Mexico'
group by country;

/* Mexico	5 */
